//1. Write a program to calculate sum of first N natural numbers.
#include<stdio.h>
#include<conio.h>
int main()
{
	int n,i,sum=0;
	printf("enter your number :- ");
	scanf("%d",&n);
	for(i=1;i<=n;i++)
	sum=sum+i;
	{
		printf("sum of first N natural numbers :- %d",sum);
		printf("\n");
	}
	return 0;
}
